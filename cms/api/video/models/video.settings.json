{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "name": "Video"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "video": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "series": {
      "model": "collection",
      "via": "videos"
    },
    "categories": {
      "collection": "category",
      "via": "videos",
      "dominant": true
    },
    "publicationDate": {
      "type": "date"
    },
    "notes": {
      "collection": "note",
      "via": "video"
    },
    "youtubeId": {
      "type": "string",
      "minLength": 10,
      "maxLength": 12
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
